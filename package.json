{
  "name": "@thebespokepixel/trucolor",
  "version": "0.0.18-alpha.47",
  "description": "24bit color tools for the commandline",
  "main": "./index.js",
  "scripts": {
    "test": "vows test/*.coffee --spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkGriffiths/trucolor.git"
  },
  "bin": {
    "trucolor": "./bin/trucolor.js"
  },
  "engines": {
    "node": ">= 0.10.32"
  },
  "keywords": [
    "color",
    "24bit",
    "SGR",
    "ansi",
    "command line",
    "fish"
  ],
  "author": "Mark Griffiths",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkGriffiths/trucolor/issues"
  },
  "homepage": "https://github.com/MarkGriffiths/trucolor",
  "devDependencies": {
    "@thebespokepixel/grunt-coffeescript": "^1.1.0",
    "coffee-script": "^1.10.0",
    "grunt": "^0.4.5",
    "grunt-bump": "^0.7.0",
    "grunt-force-task": "^1.0.0",
    "grunt-shell": "^1.1.1",
    "grunt-text-replace": "^0.4.0",
    "grunt-version": "^1.0.0",
    "load-grunt-tasks": "^3.2.0",
    "vows": "^0.8.1"
  },
  "dependencies": {
    "@thebespokepixel/verbosity": "0.0.23",
    "less": "^2.5.1",
    "supports-color": "^3.1.2",
    "truwrap": "*",
    "underscore": "^1.8.3",
    "yargs": "^3.25.0"
  },
  "xo": {
